train_net: "train.prototxt"
base_lr: 0.001
momentum: 0.9
weight_decay: 0.0005
lr_policy: "fixed"
gamma: 0.999
display: 1
# The maximum number of iterations
max_iter: 40000
# snapshot intermediate results
snapshot: 1000
snapshot_prefix: "Models/pafifa"
# solver mode: CPU or GPU
solver_mode: GPU
device_id: 1
